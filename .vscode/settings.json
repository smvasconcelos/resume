{
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit",
      "source.fixAll.stylelint": "explicit"
    },
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
    ],
    "stylelint.validate": ["typescript"],
    "editor.rulers": [80],
    "typescript.tsdk": "node_modules/typescript/lib",
    "eslint.runtime": "node",
    "cSpell.words": ["Zxcvbn"]
  }
